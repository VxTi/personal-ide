{
    "extension_name": "HTML",
    "extension_types": ["html", "xhtml", "htmx"],
    "grammar": {
        "comment": {
            "priority": 0,
            "patterns": [
                "<!--[^-->]-->"
            ]
        },
        "tag": {
            "priority": 1,
            "patterns": [
                "<[^>]*>"
            ]
        }
    }
}