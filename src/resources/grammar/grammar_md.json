{
    "extension_name": "Markdown",
    "extension_types": ["md", "markdown"],
    "grammar": {
        "chars": {
            "priority": 1,
            "patterns": [
                "[*\\-#]+"
            ]
        },
        "heading":
        {
            "priority": 2,
            "patterns": [
                "(?<=(# )).*"
            ]
        }
    }
}